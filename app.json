{
  "name": "Captainhub",
  "description": "Github Hook server (code review etc)",
  "repository": "https://github.com/orktes/captainhub",
  "keywords": ["github", "review", "golang", "github-hooks"],
  "env": {
    "GITHUB_ACCESS_TOKEN": {
      "description": "Github access token"
    },
    "GITHUB_HOOK_SECRET": {
      "description": "Hook secret"
    }
  },
  "addons": [
    "rediscloud"
  ],
  "image": "heroku/go:1.5"
}
